{{#*inline "page"}}

<div class="container-fluid">
  <h1>Tournament List</h1>
  <table class="table-bordered">
    {{#each data}}
      <tr>
        <td>{{ this.name }}</td>
        <td>{{ this.description }}</td>
        <td>["{{ this.start_date }}", "{{this.end_date}}"]</td>
      </tr>
    {{/each}}
  </table>
</div>
<script src="/tournaments.js"></script>

{{/inline}}
{{~> (parent)~}}